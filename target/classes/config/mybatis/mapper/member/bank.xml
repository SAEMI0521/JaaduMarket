<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bank">

	<insert id="insert">
		insert into BANK values(BANK_SEQ.nextval, #{BANK_NAME})
	</insert>
	
	<select id="selectList" resultType="bank">
		select * from BANK order by BANK_SEQ
	</select>


</mapper>








































